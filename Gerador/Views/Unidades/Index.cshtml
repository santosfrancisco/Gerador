@model IEnumerable<Gerador.Models.Unidades>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Empreendimentos.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Numero)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnidadeStatus)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnidadeObservacao)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Empreendimentos.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Numero)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnidadeStatus)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnidadeObservacao)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDUnidade }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDUnidade }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDUnidade })
        </td>
    </tr>
}

</table>
